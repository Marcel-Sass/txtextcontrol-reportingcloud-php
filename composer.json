{
    "name": "textcontrol/txtextcontrol-reportingcloud",
    "description": "PHP wrapper for ReportingCloud Web API. Authored and supported by Text Control GmbH.",
    "license": "BSD-3-Clause",
    "type": "library",
    "keywords": [
        "reporting cloud",
        "reportingcloud"
    ],
    "autoload": {
        "psr-4": {
            "TxTextControl\\ReportingCloud\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TxTextControlTest\\ReportingCloud\\": "test/"
        }
    },
    "require": {
        "php": "^7.1",
        "guzzlehttp/guzzle": "^6.3",
        "phan/phan": "^1.2",
        "webmozart/assert": "^1.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.11.2",
        "phpunit/phpunit": "^7.5",
        "riimu/kit-phpencoder": "^2.4",
        "smalot/pdfparser": "^0.9",
        "squizlabs/php_codesniffer": "^3.3",
        "symfony/var-dumper": "^4.2",
        "vimeo/psalm": "^3.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "phpcs": "vendor/bin/phpcs data demo src test --standard=phpcs.xml",
        "phpstan": "vendor/bin/phpstan analyse src test --level max",
        "psalm": "vendor/bin/psalm --config=psalm.xml"
    }
}
